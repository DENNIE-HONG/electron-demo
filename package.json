{
  "name": "electron-demo",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DENNIE-HONG/electron-demo.git"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "author": "luyanhong",
  "scripts": {
    "dev": "webpack --env.production=false --config build/webpack.dev.config.js",
    "devServer": "webpack-dev-server --env.production=false --config build/webpack.dev.config.js",
    "build": "webpack --env.production --config build/webpack.prod.config.js",
    "build:sit-preview": "npm_config_report=true webpack --env.production --config build/webpack.prod.config.js",
    "build-mac": "electron-builder -mwl",
    "pack-mac": "npm run build && npm run build-mac"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "throttle-debounce": "^2.0.1",
    "vanilla-lazyload": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/prop-types": "^15.5.8",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "electron-builder": "^21.0.15",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "build": {
    "productName": "electron-react",
    "appId": "electron-react",
    "asar": false,
    "files": [
      "package.json",
      "main.js",
      "node_modules/",
      "icon.ico"
    ],
    "npmRebuild": false,
    "mac": {
      "target": "dmg",
      "icon": "icon.ico"
    }
  }
}
